{
  "expo": {
    "name": "YoutubeDL",
    "slug": "youtube-dl",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/youtube.png",
    "splash": {
      "image": "./assets/splash3.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "youtube.download.ydld"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/youtube.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "youtube.download.ydld"
    },
    "web": {
      "favicon": "./assets/youtube.png"
    }
  }
}
